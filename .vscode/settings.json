{
  "cSpell.words": [
    "atrule",
    "darkgradient",
    "darknone",
    "Grotesk",
    "tailwindcss"
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": [
    "source.fixAll.eslint",
    "source.addMissingImports"
  ],
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
